@model AboutVM
<div class="page-heading">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <h1>About Us</h1>
                <div class="line"></div>
                <span>
                    Praesent volutpat nisi sed imperdiet facilisis felis turpis
                    fermentum lectus
                </span>
                <div class="pages">
                    <ul>
                        <li>
                            <a href="http://demo.mypsolution.com/wp13/">Home</a>
                        </li>
                        <li><i class="fa fa-dot-circle-o"></i></li>
                        <li>
                            <a href="http://demo.mypsolution.com/wp13/services/">About Us</a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>
<div id="page">
    <section class="about-text section">
        <div class="row-container">
            <div class="wrap-columns">
                <div class="kc-column">
                    <div>
                        <div class="kc-row-inner">
                            <div class="kc-column-inner">
                                <div class="left-about">
                                    <div>
                                        <h2>@Model.AboutText.Title</h2>
                                        <p><span>@Model.AboutText.SubTitle</span></p>
                                        <p>
                                          @Model.AboutText.Text.Substring(0,250)
                                        </p>
                                        <p>
                                            @Model.AboutText.Text.Substring(250,Model.AboutText.Text.Length-250)
                                        </p>
                                        <p><a href="#">Read More</a></p>
                                    </div>
                                </div>
                            </div>
                            <div class="kc-column-inner">
                                <div class="right-about">
                                    @foreach(var item in Model.AboutSkills.Take(5)){
                                    <div class="skillbar clearfix">
                                        <div class="skillbar-title">
                                            <h6>@item.Name</h6>
                                            <span>(@item.Degree%)</span>
                                        </div>
                                        <div class="skillbar-bar" style="width:@item.Degree%"></div>
                                    </div>
                                    }
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <section class="team-member section">
        <div class="row-container">
            <div class="wrap-columns">
                <div class="kc-column">
                    <div class="kc-col-container">
                        <div class="header text-center">
                            <h2>Meet the team</h2>
                            <span>
                                Vivamus gravida magna massa in cursus mi vehicula at.
                                Nunc sem quam suscipit
                            </span>
                            <div>
                                <i class="fa fa-user"></i>
                            </div>
                            <div class="line"></div>
                        </div>
                        <div class="team-cards kc-row-inner">
                            @foreach(var item in Model.Staffs.Take(4)){
                            <div class="kc-column-inner">
                                <div>
                                    <div class="team-member text-center">
                                        <img alt=""
                                             src="~/Images/Staffs/@item.Image" style="height:220px; width:220px;"/>
                                        <div class="socials">
                                            <div class="line"></div>
                                            <p>@item.Info</p>
                                            <ul>
                                                    @foreach(var social in item.Socials.Where(x=>!x.IsDeleted).Take(4))
                                                    {
                                                        <li>
                                                            <a href="@social.Link">
                                                                <i class="fa fa-@social.Icon"></i>
                                                            </a>
                                                        </li>
                                                    }
                                            </ul>
                                        </div>
                                        <div class="info">
                                            <h2>@item.FullName</h2>
                                            <span>@item.Position.Name</span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            }
                         
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
